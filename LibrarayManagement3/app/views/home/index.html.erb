<h1>List Of All Books</h1>
<table  style ="border: 1px solid black;">
	<tr>
		<th>name</th>
		<th>Author</th>
		<th>Price</th>
	</tr>
	<tr>
    <% @books.each do|book| %>
      <td><%= book.name%></td>
      <td><%= book.Author%></td>
      <td><%= book.Price%></td>
      <td><%= link_to "edit","home/#{book.id}/edit"%></td>
      <td><%=link_to "delete","home/#{book.id}/delete"%> </td>
    </tr>
    <%end%>
  
</table><%=link_to "createNewBook",new_home_path%>


