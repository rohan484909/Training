
<h1>Create New User</h1>
<%= form_with(model:@student, local: true) do |form| %>
  <div class="field">
    <%= form.label :username %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.text_field :password %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<%end%>
