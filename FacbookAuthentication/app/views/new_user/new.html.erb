<h1>Create New User</h1>
  <%= form_with model: @user,action:, local: true do |form| %>
      <%= form.label "name"%>
      <%= form.text_field :name %><br/>
      <%= form.label "email"%>
      <%= form.text_field :email%><br/>
      <%= form.label "password"%>
      <%= form.text_field :password1%><br/>
      <%= form.label "password"%>
      <%= form.text_field :password2 %>
      <%= form.submit %>
  <% end %>