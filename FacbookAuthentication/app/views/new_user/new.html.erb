<h1>Create New User</h1>
  <%= form_with model: @user,url: {action:"create"} , local: true do |form| %>
      <%= form.label "name"%>
      <%= form.text_field :name %><br/>
      <%= form.label "email"%>
      <%= form.text_field :email%><br/>
      <%= form.label "password"%>
      <%= form.password_field :password%><br/>
      
      <%= form.submit %>
  <% end %>